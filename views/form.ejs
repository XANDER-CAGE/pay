<script>
  window.onload = submitData;

  function submitData() {
    const url = "<%= TermUrl %>";
    const data = {
      PaRes: "<%= PaRes %>",
      MD: "<%= md %>"
    };

    fetch(url, {
      method: "POST",
      headers: {
        "Content-Type": "application/json"
      },
      body: JSON.stringify(data)
    })
    .then(response => response.json())
    .then(data => console.log(data))
    .catch(error => console.error('Error:', error));
  }
</script>
